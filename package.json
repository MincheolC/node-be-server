  },
  "homepage": "https://github.com/MincheolC/node-be-server#readme",
  "devDependencies": {
    "eslint": "^8.28.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.20",
    "prettier": "2.7.1",
    "sequelize-cli": "^6.5.2"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "sequelize": "^6.25.7"
  }
}
